    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "setup": "npm install",
    "sqlite:backup": "bash scripts/database/sqlite-backup.sh",
    "sqlite:restore": "bash scripts/database/sqlite-restore.sh",
    "test": "vitest run",
    "test:watch": "vitest",
    "evals": "RAH_EVALS_LOG=1 tsx tests/evals/runner.ts"
  },
